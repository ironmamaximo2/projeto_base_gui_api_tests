
pipeline {
    agent { dockerfile true{
      additionalBuildArgs "--build-arg user_id=${user_id} --build-arg group_id=${group_id}"
    } }
    stages {
        stage('Change api_url in Cypress') {
            steps {

                sh 'npm run altUrlApiCypressJenkins'
            }
        }
        stage('Test E2E') {
            steps {
                sh 'node --version'
                sh 'npm run test_all'
            }
        }
    }
}