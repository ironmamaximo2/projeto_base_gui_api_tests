
pipeline {
    agent { dockerfile true }
    stages {
        stage('Change api_url in Cypress') {
            steps {

                sh 'docker exec -u 0 -it mycontainer bash'
                sh 'npm run altUrlApiCypressJenkins'
            }
        }
        stage('Test E2E') {
            steps {
                sh 'node --version'
                sh 'npm run test_all'
            }
        }
    }
}