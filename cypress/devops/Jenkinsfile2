
pipeline {
  agent any
 
  tools {nodejs "node"}
 
 
    stage('install') {
      steps {
        sh 'npm install'
      }
    }

    stage('cypress open') {
      steps {
        sh 'npx cypress open'
      }
    }

    stage('test') {
      steps {
        sh 'npm run test_all'
      }
    }
  }
}