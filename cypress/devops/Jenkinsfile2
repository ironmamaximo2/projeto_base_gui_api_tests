pipeline {
    agent {
        docker { image 'node:14-alpine' }
        
    }
 
  stages {
    stage('Example') {
      steps {
        sh 'npm config ls'
      }
    }
    stage('install') {
      steps {
        sh 'npm install'
      }
    }

    stage('cypress open') {
      steps {
        sh 'npx cypress open'
      }
    }

    stage('test') {
      steps {
        sh 'npm run testC'
      }
    }
  }
}